<p id="notice"><%= notice %></p>

<h1>Actor Skills</h1>

<table>
  <thead>
    <tr>
      <th>Special skill</th>
      <th>Actor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @actor_skills.each do |actor_skill| %>
      <tr>
        <td><%= actor_skill.special_skill_id %></td>
        <td><%= actor_skill.actor_id %></td>
        <td><%= link_to 'Show', actor_skill %></td>
        <td><%= link_to 'Edit', edit_actor_skill_path(actor_skill) %></td>
        <td><%= link_to 'Destroy', actor_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Actor Skill', new_actor_skill_path %>

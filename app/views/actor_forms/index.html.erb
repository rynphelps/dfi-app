<p id="notice"><%= notice %></p>

<h1>Actor Forms</h1>

<table>
  <thead>
    <tr>
      <th>Other forms link</th>
      <th>Actor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @actor_forms.each do |actor_form| %>
      <tr>
        <td><%= actor_form.other_forms_link_id %></td>
        <td><%= actor_form.actor_id %></td>
        <td><%= link_to 'Show', actor_form %></td>
        <td><%= link_to 'Edit', edit_actor_form_path(actor_form) %></td>
        <td><%= link_to 'Destroy', actor_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Actor Form', new_actor_form_path %>

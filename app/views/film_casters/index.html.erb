<p id="notice"><%= notice %></p>

<h1>Film Casters</h1>

<table>
  <thead>
    <tr>
      <th>Casting director</th>
      <th>Film</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @film_casters.each do |film_caster| %>
      <tr>
        <td><%= film_caster.casting_director_id %></td>
        <td><%= film_caster.film_id %></td>
        <td><%= link_to 'Show', film_caster %></td>
        <td><%= link_to 'Edit', edit_film_caster_path(film_caster) %></td>
        <td><%= link_to 'Destroy', film_caster, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Film Caster', new_film_caster_path %>
